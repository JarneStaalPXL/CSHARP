<Window x:Class="Taak_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Taak_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>


        <StackPanel Grid.Row="0">
            <TextBox x:Name="outputBox" Height="201"  />
        </StackPanel>
        <StackPanel Grid.Row="1" Margin="50">
            <Button Click="Inlezen">Inlezen</Button>
            <Button Click="Opslaan">Opslaan</Button>
        </StackPanel>
        
    </Grid>
</Window>
