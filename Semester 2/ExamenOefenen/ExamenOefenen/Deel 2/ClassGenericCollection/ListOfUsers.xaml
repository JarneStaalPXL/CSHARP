<Window x:Class="ExamenOefenen.Deel_2.ClassGenericCollection.ListOfUsers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamenOefenen.Deel_2.ClassGenericCollection"
        mc:Ignorable="d"
        Title="ListOfUsers" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="usernameBox" HorizontalAlignment="Left" Margin="270,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="245" Height="37"/>
        <Button Content="ADD" HorizontalAlignment="Left" Margin="270,201,0,0" VerticalAlignment="Top" Width="245" Height="36" Click="Add_Click"/>
        <Button Content="SHOW" HorizontalAlignment="Left" Margin="270,237,0,0" VerticalAlignment="Top" Width="245" Height="36" Click="Show_Click"/>

        <Label Content="USERNAME" HorizontalAlignment="Left" Margin="184,119,0,0" VerticalAlignment="Top" Height="37"/>
        <ListBox x:Name="lstBox" Margin="0,285,0,0"/>

    </Grid>
</Window>
